{
	"name": "CountDownClock",
	"author": [
		"[https://meta.wikimedia.org/wiki/Indic-TechCom Indic-TechCom]"
	],
	"version": "1.0.0",
	"url": "https://www.mediawiki.org/wiki/Extension:CountDownClock",
	"descriptionmsg": "countDownClock-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.29.0"
